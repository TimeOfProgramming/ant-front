{
  "name": "ant_front",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "api:generate": "npx swagger-typescript-api --path https://back.dev.apexnovatech.com/docs-yaml --output src/shared/api/artifacts --name generated.ts --responses --sort-types --sort-routes --axios --unwrap-response-data",
    "build": "vite build",
    "dev": "vite",
    "format": "prettier --list-different .",
    "format:fix": "prettier --write --list-different .",
    "lint": "eslint *.js *.mjs *.ts src --report-unused-disable-directives",
    "lint:fix": "npm run lint -- --fix",
    "optimize:svg": "svgo public src/shared/assets --recursive",
    "prepare": "husky",
    "preview": "vite preview",
    "stylelint": "stylelint  \"src/**/*.css\" --ignore-path .gitignore --allow-empty-input",
    "stylelint:fix": "npm run stylelint -- --fix",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.2.5",
    "@hookform/resolvers": "^5.0.1",
    "@tanstack/react-query": "^5.76.1",
    "axios": "^1.9.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "i18next": "^25.1.3",
    "i18next-browser-languagedetector": "^8.1.0",
    "jwt-decode": "^4.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.4",
    "react-i18next": "^15.5.1",
    "react-router": "^7.6.0",
    "react-router-dom": "^7.6.0",
    "validator": "^13.15.0",
    "zod": "^3.24.4",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@csstools/postcss-global-data": "^3.0.0",
    "@eslint/js": "^9.27.0",
    "@types/date-fns": "^2.6.3",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@types/validator": "^13.15.0",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.3.5",
    "eslint-plugin-i18n-json": "^4.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^4.13.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "postcss-preset-env": "^10.1.6",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.13",
    "stylelint": "^16.19.1",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-use-logical-spec": "^5.0.1",
    "svgo": "^3.3.2",
    "swagger-typescript-api": "^13.1.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.32.1",
    "typescript-plugin-css-modules": "^5.1.0",
    "validate-branch-name": "^1.3.2",
    "vite": "^6.3.5",
    "vite-plugin-mkcert": "^1.17.8",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
